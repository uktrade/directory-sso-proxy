{
  "host_env_var_prefix": "SSO_PROXY",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "SIGNATURE_SECRET",
      "SSO_SIGNATURE_SECRET",
      "SSO_UPSTREAM"
    ],
    "optional": [
      "DEBUG",
      "SECURE_HSTS_SECONDS",
      "PYTHONWARNINGS",
      "PYTHONDEBUG",
      "SECURE_SSL_REDIRECT"
    ]
  }
}
